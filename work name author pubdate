SELECT (split_part(to_char(adobe_sc_omniture_hub_v3.date_time::timestamp without time zone, 'YYYY-MM-DD'::text),'-',1)||'-'||split_part(to_char(adobe_sc_omniture_hub_v3.date_time::timestamp without time zone, 'YYYY-MM-DD'::text),'-',2)) AS month, adobe_sc_omniture_hub_v3.post_prop3_article_blog_title AS Aricle_title, adobe_sc_omniture_hub_v3.post_prop6_article_blog_author AS author , to_char(adobe_sc_omniture_hub_v3.post_prop5_article_blog_date::timestamp without time zone, 'YYYY-MM-DD'::text) AS publishdate , count(Distinct adobe_sc_omniture_hub_v3.post_prop3_article_blog_title) AS Articles
FROM omniture_mt.adobe_sc_omniture_hub_v3 
WHERE adobe_sc_omniture_hub_v3.exclude_hit::text = '0'::text AND adobe_sc_omniture_hub_v3.hit_source::integer <> 5 AND adobe_sc_omniture_hub_v3.hit_source::integer <> 8 AND adobe_sc_omniture_hub_v3.hit_source::integer <> 9 and to_char(adobe_sc_omniture_hub_v3.post_prop5_article_blog_date::timestamp without time zone, 'YYYY-MM-DD'::text) like '___6-03-__'
GROUP BY month, Aricle_title, author, publishdate;
